
#ifndef UART1_H_
#define UART1_H_
#include "stm32f103xb.h"
#define GPIOAEN (1U<<2)
#define UART1EN   (1U << 14)
#define UART_SR_TXE    (1U << 7)
#define UART_SR_RXNE   (1U << 5)

void uart1_init(void);
void uart1_write(int ch);
char uart1_read(void);
void uart1_write_string(const char *str);

#endif /* UART1_H_ */
